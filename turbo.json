{
  "globalEnv": [".env"],
  "globalDependencies": [],
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "migrate": {
      "cache": false
    },
    "build": {
      "dependsOn": ["migrate", "^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "dependsOn": ["^clean"]
    }
  }
}
